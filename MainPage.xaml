<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MenuApp"
             xmlns:ext="clr-namespace:MenuApp.Extensions"
             x:Class="MenuApp.MainPage">

    <ext:CustomFilesBlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <ext:CustomFilesBlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type local:Components.Routes}" />
        </ext:CustomFilesBlazorWebView.RootComponents>
    </ext:CustomFilesBlazorWebView>

</ContentPage>
